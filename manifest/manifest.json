{
  "manifest_version": 2,
  "name": "server360.io Extension",
  "version": "0.5.16",
  "description": "Browser extension for server360.io",
  "author": "Tom S. <thoschulte@gmail.com>",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "16": "assets/icon16.png",
    "32": "assets/icon32.png"
  },
  "page_action": {
    "default_title": "Page Action TomS."
  },
  "permissions": [
    "declarativeContent",
    "activeTab",
    "history"
  ]
}
